set nocompatible
set encoding=utf-8
syntax on
filetype plugin on
filetype plugin indent on
set ruler
set complete-=i
set nrformats-=octal
set autoindent
set autoread
set backspace=indent,eol,start
set smarttab
set incsearch
set laststatus=2
set display+=lastline
set wildmenu
set tabpagemax=50
set sessionoptions-=options
